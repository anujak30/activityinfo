<module rename-to="App">
	<inherits name='com.google.gwt.user.User' />	
	<inherits name="org.activityinfo.ActivityInfoCore"/>
	<inherits name="org.activityinfo.login.LoginApi"/>
	
    <entry-point class="org.activityinfo.chrome.client.ChromeAppEntryPoint"/>

	<source path='client' />
	<source path="shared" />
	
	<set-property name="user.agent" value="safari"/>
	<collapse-property name="locale" values="*" />
	
</module>