<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

  <changeSet author="alex" id="018-01">
    <comment>
      Batch import  
    </comment>
    
    <!-- We originally added a unique constraint on the version field because we batched
         sync updates by version and it was difficult to do so reliably when there were multiple
         rows with the same timestamp (version). But batching wasn't necessary in the end and 
         the unique contrainst interferes with batch importing  -->
    <dropUniqueConstraint tableName="location" constraintName="uniqueVersionKey"/>
    
  </changeSet>
  
  
</databaseChangeLog>